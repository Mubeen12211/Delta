{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api.py"
    }
  ],
  "build": {
    "env": {
      "GOOGLE_CHROME_SHIM": "/usr/bin/google-chrome-stable"
    },
    "commands": {
      "build": "bash build.sh"
    }
  }
}
